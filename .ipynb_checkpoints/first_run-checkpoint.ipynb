    "sql_beer = sqlite3.connect('beers.db')\n",
    "\n",
    "# ------------------------------#\n",
    "# Display Highest ABV American Pale Ales\n",
    "high_abv_APA =pd.read_sql_query(\n",
    "    'SELECT abv, name, style, brewery_id FROM beer WHERE abv > 0.01 AND style LIKE \"American Pale Ale%\" ORDER BY abv DESC LIMIT 10'\n",
